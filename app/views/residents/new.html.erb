<h1>New Resident</h1>

<%= form_for @resident do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name, :required => true%>
    <%= f.label :age %>
    <%= f.number_field :age, :required => true%>
    <%= f.label :room %>
    <%= f.select :room, options_for_select(@all_rooms) %>
    <%= f.submit %>
<%end%>
